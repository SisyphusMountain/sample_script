newick = { subtree ~ ";" }

subtree = { leaf | internal }
leaf = { NAME? ~ ":" ~ LENGTH? }
internal = { "(" ~ subtree ~ "," ~ subtree ~ ")" ~ NAME? ~ ":"? ~ LENGTH? }

NAME = @{ ASCII_ALPHANUMERIC+ }
LENGTH = @{ (ASCII_DIGIT+ ~ ("." ~ ASCII_DIGIT*)? ~ ("e" | "E") ~ ("+" | "-")? ~ ASCII_DIGIT+) | (ASCII_DIGIT+ ~ "." ~ ASCII_DIGIT*) | (ASCII_DIGIT+) }
